<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SlideshowController">
   <children>
      <AnchorPane layoutX="-2.0" layoutY="-1.0" prefHeight="525.0" prefWidth="606.0">
         <children>
            <Label layoutX="252.0" layoutY="26.0" text="Slideshow">
               <font>
                  <Font size="25.0" />
               </font></Label>
            <Button fx:id="quit_btn" layoutX="524.0" layoutY="484.0" mnemonicParsing="false" onAction="#quitBtnAction" text="Quit" />
            <Button fx:id="logout_btn" layoutX="452.0" layoutY="484.0" mnemonicParsing="false" onAction="#logoutBtnAction" text="Logout" />
            <Button fx:id="back_btn" layoutX="393.0" layoutY="484.0" mnemonicParsing="false" onAction="#backBtnAction" text="Back" />
            <Button fx:id="next_btn" layoutX="521.0" layoutY="175.0" mnemonicParsing="false" onAction="#nextImage" text="Next" />
            <Button fx:id="prev_btn" layoutX="22.0" layoutY="175.0" mnemonicParsing="false" onAction="#prevImage" text="Previous" />
            <ImageView fx:id="img_slide" fitHeight="341.0" fitWidth="340.0" layoutX="136.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="cap_lbl" layoutX="136.0" layoutY="438.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
